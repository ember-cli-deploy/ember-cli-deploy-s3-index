{
  "name": "ember-cli-deploy-s3-index",
  "version": "1.1.0",
  "description": "Ember CLI Deploy plugin to deploy ember-cli's bootstrap index file to S3.",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "release": "release-it",
    "test": "node tests/runner.js && ./node_modules/.bin/eslint index.js lib/* tests/**/*.js"
  },
  "repository": "https://github.com/ember-cli-deploy/ember-cli-deploy-s3-index",
  "engines": {
    "node": ">= 4"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "github": "^0.2.4",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "ember-cli": "~3.18.0",
    "eslint": "^7.0.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.2",
    "release-it": "*"
  },
  "keywords": [
    "ember-addon",
    "ember-cli-deploy-plugin"
  ],
  "dependencies": {
    "aws-sdk": "^2.667.0",
    "core-object": "^3.0.0",
    "ember-cli-deploy-plugin": "^0.2.9",
    "mime-types": "^2.1.27",
    "rsvp": "^4.8.5"
  },
  "release-it": {
    "git": {
      "changelog": "./bin/changelog",
      "requireCleanWorkingDir": false
    },
    "github": {
      "release": true
    }
  }
}
